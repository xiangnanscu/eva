{
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "NITRO_PORT=3001 nuxt preview",
    "start": "pm2 start --env production",
    "stop": "pm2 stop eva",
    "reload": "pm2 reload --update-env eva",
    "restart": "pm2 delete eva && yarn start",
    "restart2": "pm2 restart eva",
    "deploy": "git add . && git commit -am \"${msg:-deploy}\" && git push prod master -o build -o restart2",
    "db": "prisma db push",
    "pmd": "prisma migrate deploy",
    "push": "git add . && git commit -am x && git push origin master",
    "db:init": "npx prisma migrate dev --name init",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "nuxt": "3.0.0-rc.11",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "dotenv": "^16.0.2"
  }
}
